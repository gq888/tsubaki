{
  "name": "tsubaki-game-library",
  "version": "1.0.0",
  "description": "高效可复用的Vue游戏组件库，基于工厂函数模式实现极简开发体验",
  "keywords": [
    "vue",
    "game",
    "components",
    "factory-pattern",
    "reusable"
  ],
  "author": "Tsubaki Team",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/tsubaki/game-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsubaki/game-library.git"
  },
  "bugs": {
    "url": "https://github.com/tsubaki/game-library/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "migrate": "node scripts/migrate-components.js migrate",
    "migrate:dry-run": "node scripts/migrate-components.js migrate --dry-run",
    "migrate:rollback": "node scripts/migrate-components.js rollback",
    "migrate:report": "node scripts/migrate-components.js report",
    "test:refactored": "node scripts/test-refactored-components.js all",
    "test:performance": "node scripts/test-refactored-components.js all --performance",
    "validate:components": "node scripts/migrate-components.js validate",
    "generate": "node scripts/generate-component.js interactive",
    "generate:quick": "node scripts/generate-component.js quick",
    "generate:examples": "node scripts/generate-component.js examples",
    "perf:complexity": "node scripts/performance-monitor.js complexity",
    "perf:build": "node scripts/performance-monitor.js build",
    "perf:report": "node scripts/performance-monitor.js report",
    "perf:monitor": "node scripts/performance-monitor.js monitor",
    "deps:analyze": "node scripts/dependency-analyzer.js analyze",
    "deps:visualize": "node scripts/dependency-analyzer.js visualize",
    "deps:mermaid": "node scripts/dependency-analyzer.js mermaid",
    "dev:optimize": "node scripts/dev-optimizer.js optimize",
    "dev:analyze": "node scripts/dev-optimizer.js analyze",
    "status": "node scripts/project-status.js full",
    "status:quick": "node scripts/project-status.js quick",
    "health": "node scripts/project-status.js quick && echo '\\n🎮 Tsubaki游戏组件库运行正常！'",
    "dev": "npm run serve",
    "start": "npm run serve",
    "clean": "rimraf dist node_modules/.cache .eslintcache",
    "clean:install": "npm run clean && npm install",
    "prebuild": "npm run lint",
    "postbuild": "npm run perf:build",
    "check": "npm run lint && npm run test:refactored",
    "check:all": "npm run check && npm run perf:report && npm run deps:analyze",
    "setup": "npm install && npm run dev:optimize",
    "demo": "npm run generate:examples && npm run serve",
    "docs": "echo 'Opening documentation...' && open COMPONENT_LIBRARY.md",
    "welcome": "node scripts/welcome.js",
    "help": "node -e \"console.log('\\n🎮 Tsubaki游戏组件库\\n\\n常用命令:\\n  npm run dev        - 启动开发服务器\\n  npm run generate   - 创建新游戏组件\\n  npm run check:all  - 全面检查\\n  npm run docs       - 查看文档\\n  npm run welcome    - 显示欢迎指南\\n\\n更多命令请查看 package.json\\n')\""
  },
  "dependencies": {
    "@vue/test-utils": "^2.4.1",
    "core-js": "^3.6.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@babel/eslint-parser": "^7.22.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "prettier": "^3.0.0",
    "rimraf": "^3.0.2",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "requireConfigFile": false
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "vue/multi-word-component-names": "off",
      "vue/no-unused-components": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "tsubaki": {
    "version": "1.0.0",
    "components": {
      "refactored": 6,
      "presets": 7,
      "codeReduction": "771 lines (55.8%)"
    },
    "tools": {
      "scripts": 15,
      "commands": 27,
      "features": [
        "migration",
        "generation",
        "testing",
        "performance",
        "dependencies",
        "optimization"
      ]
    },
    "documentation": {
      "library": "COMPONENT_LIBRARY.md",
      "migration": "MIGRATION_GUIDE.md",
      "summary": "PROJECT_COMPLETE.md"
    }
  }
}
